{
  "name": "ts-engineer",
  "version": "1.0.0",
  "description": "A web app to do AI assisted coding",
  "main": "index.js",
  "type": "module",
  "imports": {
    "~/*": "./*"
  },
  "scripts": {
    "dev": "bun run tasks/dev.ts",
    "dev:app": "vite",
    "dev:api": "bun run --hot api/server.ts",
    "build": "remix vite:build",
    "start:app": "remix-serve build/server/index.js",
    "start:api": "bun run api/server.ts",
    "start": "bun run tasks/start.ts",
    "test": "bun test",
    "backup": "bun run tasks/backup.ts",
    "restore": "bun run tasks/restore.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DroopyTersen/ts-engineer.git"
